{
  "private": true,
  "name": "sanity-ui-workshop",
  "version": "1.0.47",
  "main": "package.json",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf build dist src/packages.js src/scopes.js",
    "dev": "vite",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "prebuild": "yarn clean",
    "scopes:build": "node -r esbuild-register scripts/scopes/build.ts",
    "start": "http-server -a localhost -c-0 -p 9009 -s -P http://localhost:9009/index.html? dist",
    "tsc": "tsc --build",
    "type-check": "run-s scopes:build tsc"
  },
  "dependencies": {
    "@sanity/ui-workshop": "^0.6.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refractor": "^2.1.7",
    "refractor": "^4.8.0",
    "styled-components": "6.0.0-beta.3"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/refractor": "^3.0.2",
    "@vitejs/plugin-react": "^2.1.0",
    "chokidar": "^3.5.3",
    "vite": "^3.1.6"
  }
}
