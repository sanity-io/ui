{
  "private": true,
  "name": "sanity-design-docs",
  "version": "0.1.104",
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next build packages.js workshop/scopes.js",
    "dev": "run-s packages:build scopes:build && run-p scopes:watch next:dev",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "next:dev": "next dev",
    "packages:build": "node -r esbuild-register scripts/packages/build.ts",
    "prebuild": "run-s clean packages:build scopes:build",
    "scopes:build": "node -r esbuild-register scripts/scopes/build.ts",
    "scopes:watch": "node -r esbuild-register scripts/scopes/watch.ts",
    "start": "next start",
    "test": "vitest run",
    "tsc:build": "tsc --build",
    "type-check": "run-s packages:build scopes:build tsc:build"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/color": "^2.1.15",
    "@sanity/icons": "^1.3.5",
    "@sanity/image-url": "^1.0.1",
    "@sanity/logos": "^1.1.16",
    "@sanity/ui": "^0.38.1",
    "@sanity/ui-workshop": "^0.6.0",
    "codemirror": "^5.65.2",
    "date-fns": "^2.28.0",
    "lodash-es": "^4.17.21",
    "next": "^12.2.5",
    "next-sanity": "0.8.1",
    "pako": "^2.0.4",
    "qs": "^6.11.0",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.5",
    "@types/lodash-es": "^4.17.6",
    "@types/pako": "^2.0.0",
    "@types/qs": "^6.9.7",
    "babel-plugin-styled-components": "^2.0.7",
    "chokidar": "^3.5.3",
    "next-transpile-modules": "^9.0.0"
  }
}
