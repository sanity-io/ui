{
  "name": "@sanity/ui",
  "version": "1.0.0-alpha.6",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-p build-*",
    "build-cjs": "tsc --module CommonJS --outDir dist/cjs --project tsconfig.dist.json --rootDir ./src --target ES5",
    "build-es": "tsc --module ES6 --outDir dist/es --project tsconfig.dist.json --rootDir ./src --target ES6",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "watch": "run-p watch-cjs watch-es",
    "watch-cjs": "tsc -w --module CommonJS --outDir dist/cjs --project tsconfig.dist.json --rootDir ./src --target ES5",
    "watch-es": "tsc -w --module ES6 --outDir dist/es --project tsconfig.dist.json --rootDir ./src --target ES6"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.2.0",
    "@reach/auto-id": "^0.11.2",
    "@sanity/color": "2.0.1",
    "@sanity/icons": "1.0.0-alpha.3",
    "polished": "^4.0.3"
  },
  "devDependencies": {
    "@popperjs/core": "^2.4.4",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@types/react-dom": "^16.9.8",
    "@types/react-is": "^16.7.1",
    "@types/refractor": "^3.0.0",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/styled-components": "^5.1.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-popper": "^2.2.3",
    "react-refractor": "^2.1.2",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.4",
    "react": "^16.9 || ^17",
    "react-popper": "^2.2",
    "react-refractor": "^2.1",
    "styled-components": "^5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "components"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "restricted"
  }
}
