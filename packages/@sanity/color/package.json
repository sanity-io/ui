{
  "name": "@sanity/color",
  "version": "2.1.14",
  "description": "The Sanity color palette.",
  "sideEffects": false,
  "source": "./src/index.ts",
  "module": "./dist/sanity-color.js",
  "types": "./dist/types/src/index.d.ts",
  "main": "./dist/sanity-color.cjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/sanity-color.js",
      "types": "./dist/types/src/index.d.ts",
      "require": "./dist/sanity-color.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg build --tsconfig tsconfig.dist.json",
    "clean": "rimraf dist",
    "generate": "node -r esbuild-register scripts/generate.ts",
    "lint": "eslint . --ext .js,.ts --quiet",
    "prebuild": "yarn clean && yarn generate",
    "test": "jest",
    "type-check": "tsc --build",
    "watch": "pkg watch --tsconfig tsconfig.dist.json"
  },
  "devDependencies": {
    "polished": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/design.git"
  },
  "keywords": [
    "sanity",
    "color",
    "palette"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/design/issues"
  },
  "homepage": "https://www.sanity.io/",
  "publishConfig": {
    "access": "public"
  }
}
