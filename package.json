{
  "name": "@sanity/ui",
  "version": "4.0.0-static.35",
  "keywords": [
    "react",
    "ui",
    "kit",
    "library",
    "primitives",
    "components",
    "utils"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/ui.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "source": "./exports/index.ts",
      "default": "./dist/index.js"
    },
    "./css": {
      "source": "./exports/css.ts",
      "default": "./dist/css/index.js"
    },
    "./css/index.css": "./dist/css/index.css",
    "./package.json": "./package.json",
    "./theme": {
      "source": "./exports/theme.ts",
      "default": "./dist/theme.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "css": [
        "./dist/css.d.ts"
      ],
      "theme": [
        "./dist/theme.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "exports",
    "src"
  ],
  "scripts": {
    "build": "run-s clean pkg:build pkg:check",
    "clean": "rimraf dist",
    "commit": "cz",
    "cypress:dev": "run-p dev cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "run-p workshop:dev storybook:dev",
    "figma:build": "pnpm run --filter 'figma-plugin-sanity-ui' build",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "pkg:build": "mkdirp dist/css && touch dist/css/index.css && NODE_ENV=production pkg build --strict",
    "pkg:check": "pkg --strict",
    "pkg:watch": "mkdirp dist/css && touch dist/css/index.css && NODE_ENV=production pkg watch --strict",
    "prepare": "husky install",
    "release": "semantic-release",
    "storybook:build": "pnpm run --filter 'sanity-ui-storybook' build",
    "storybook:dev": "pnpm run --filter 'sanity-ui-storybook' dev",
    "test": "vitest run",
    "test:browser": "start-server-and-test 'run-s workshop:build workshop:preview' http://localhost:1337 'run-s cypress:run'",
    "typecheck": "tsc --build",
    "watch": "pkg watch --strict",
    "workshop:build": "workshop build",
    "workshop:clean": "rimraf .workshop",
    "workshop:dev": "run-s workshop:clean && workshop dev",
    "workshop:preview": "workshop preview"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "browserslist": "extends @sanity/browserslist-config",
  "release": {
    "branches": [
      {
        "name": "v2",
        "range": "2.x",
        "channel": "v2"
      },
      "main",
      {
        "name": "v4",
        "channel": "static",
        "prerelease": "static"
      }
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.6",
    "@juggle/resize-observer": "^3.4.0",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.4",
    "@vanilla-extract/dynamic": "^2.1.5",
    "csstype": "^3.1.3",
    "framer-motion": "^12.23.21",
    "react-refractor": "^4.0.0",
    "use-effect-event": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@sanity/browserslist-config": "^1.0.5",
    "@sanity/pkg-utils": "^8.1.16",
    "@sanity/prettier-config": "^2.0.1",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@sanity/ui-workshop": "4.0.0-static.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-is": "^19.0.0",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/browser": "^3.2.4",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "browserslist-config-baseline": "^0.5.0",
    "cac": "^6.7.14",
    "commitizen": "^4.3.1",
    "cypress": "^13.17.0",
    "cypress-real-events": "^1.14.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-gha": "^1.6.0",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "eslint-plugin-react-hooks-with-use-effect-event": "npm:eslint-plugin-react-hooks@0.0.0-experimental-83c88ad4-20250923",
    "eslint-plugin-react-refresh": "^0.4.21",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.7",
    "globals": "^16.3.0",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^14.0.1",
    "mkdirp": "^3.0.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-is": "^19.1.1",
    "refractor": "^5.0.0",
    "rimraf": "^5.0.10",
    "semantic-release": "24.2.7",
    "start-server-and-test": "^2.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-axe": "1.0.0-pre.5"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "react-is": "^19"
  },
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "cypress"
    ],
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom",
      "@types/react-is": "$@types/react-is",
      "conventional-changelog-conventionalcommits": ">= 8.0.0",
      "react": "$react",
      "react-dom": "$react-dom",
      "react-is": "$react-is"
    }
  },
  "esm.sh": {
    "bundle": false
  }
}
