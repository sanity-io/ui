{
  "name": "@sanity/ui",
  "version": "2.15.18",
  "keywords": [
    "react",
    "ui",
    "kit",
    "library",
    "primitives",
    "components",
    "utils"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/ui.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./exports/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./_visual-editing": {
      "source": "./exports/_visual-editing.ts",
      "import": "./dist/_visual-editing.js",
      "require": "./dist/_visual-editing.cjs",
      "default": "./dist/_visual-editing.js"
    },
    "./css": {
      "source": "./exports/css.ts",
      "import": "./dist/css.js",
      "require": "./dist/css.cjs",
      "default": "./dist/css.js"
    },
    "./package.json": "./package.json",
    "./theme": {
      "source": "./exports/theme.ts",
      "import": "./dist/theme.js",
      "require": "./dist/theme.cjs",
      "default": "./dist/theme.js"
    },
    "./ui-theme.css": "./dist/ui-theme.css",
    "./ui-system.css": "./dist/ui-system.css",
    "./ui.css": "./dist/ui.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "_visual-editing": [
        "./dist/_visual-editing.d.ts"
      ],
      "css": [
        "./dist/css.d.ts"
      ],
      "theme": [
        "./dist/theme.d.ts"
      ]
    }
  },
  "bin": {
    "sanity-ui": "./bin/sanity-ui.cjs"
  },
  "files": [
    "dist",
    "exports",
    "src"
  ],
  "scripts": {
    "build": "run-s clean css:build pkg:build pkg:check figma:pkg:build",
    "clean": "rimraf dist",
    "css:build": "node -r esbuild-register scripts/build",
    "commit": "cz",
    "cypress:dev": "run-p dev cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "run-p workshop:clean workshop:dev",
    "figma:pkg:build": "cd figma && pnpm build",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint .",
    "pkg:build": "pkg build --strict",
    "pkg:check": "pkg --strict",
    "prepare": "husky install",
    "prepack": "pnpm build",
    "release": "semantic-release",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "test": "vitest run",
    "test:browser": "start-server-and-test 'run-s workshop:build workshop:start' http://localhost:1337 'run-s cypress:run'",
    "typecheck": "tsc",
    "watch": "pkg watch --strict",
    "workshop:build": "workshop build",
    "workshop:clean": "rimraf .workshop",
    "workshop:dev": "node -r esbuild-register ../ui-workshop/scripts/dev",
    "workshop:start": "http-server -a localhost -c-0 -p 1337 -s -P http://localhost:1337/index.html? dist"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "prettier": "@sanity/prettier-config",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.2",
    "@juggle/resize-observer": "^3.4.0",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.0",
    "cssnano": "^7.0.7",
    "csstype": "^3.1.3",
    "framer-motion": "^12.9.7",
    "gzip-size": "^7.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "pretty-bytes": "^7.0.0",
    "react-compiler-runtime": "19.1.0-rc.2",
    "react-refractor": "^2.2.0",
    "use-effect-event": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.26.0",
    "@sanity/pkg-utils": "^7.2.2",
    "@sanity/prettier-config": "^1.0.3",
    "@sanity/ui-workshop": "^2.1.2",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-mdx-gfm": "^8.6.12",
    "@storybook/addon-storysource": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/theming": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.7",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-is": "^19.0.0",
    "@types/refractor": "^3.4.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "cac": "^6.7.14",
    "commitizen": "^4.3.1",
    "cypress": "^13.17.0",
    "cypress-real-events": "^1.14.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "^4.1.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "0.0.0-experimental-4448b187-20250515",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all2": "^5.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "refractor": "^4.9.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.2.3",
    "start-server-and-test": "^2.0.11",
    "storybook": "^8.6.12",
    "styled-components": "^6.1.17",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "vitest-axe": "1.0.0-pre.5"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "react-is": "^19",
    "styled-components": "^6"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "@sanity/ui-workshop": "file:registry/sanity-ui-workshop-3.0.0-local.4.tgz",
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom",
      "@types/react-is": "$@types/react-is",
      "conventional-changelog-conventionalcommits": ">= 8.0.0",
      "react": "$react",
      "react-dom": "$react-dom",
      "react-is": "$react-is"
    }
  },
  "esm.sh": {
    "bundle": false
  }
}
