{
  "name": "@sanity/ui",
  "version": "2.0.0-alpha.19",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "module": "./dist/index.esm.js",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "node": {
        "module": "./dist/index.esm.js",
        "import": "./dist/index.cjs.mjs"
      },
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s clean build:pkg check:pkg",
    "build:pkg": "pkg build --strict",
    "check:pkg": "pkg --strict",
    "check:types": "tsc",
    "clean": "rimraf .workshop dist",
    "commit": "cz",
    "cypress:dev": "run-p dev cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "run-s workshop:dev",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint . --ext .cjs,.js,.jsx,.mjs,.ts,.tsx --quiet",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "release": "semantic-release",
    "test": "jest",
    "test:browser": "start-server-and-test 'run-s workshop:build workshop:start' http://localhost:1337 'run-s cypress:run'",
    "watch": "pkg watch --strict",
    "workshop:build": "workshop build",
    "workshop:dev": "workshop dev",
    "workshop:start": "http-server -a localhost -c-0 -p 1337 -s -P http://localhost:1337/index.html? dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@floating-ui/react-dom": "2.0.0",
    "@sanity/color": "3.0.0-beta.6",
    "@sanity/icons": "^2.7.0",
    "csstype": "^3.1.2",
    "framer-motion": "^10.16.5",
    "react-refractor": "^2.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@juggle/resize-observer": "^3.4.0",
    "@sanity/pkg-utils": "^3.2.5",
    "@sanity/semantic-release-preset": "^4.1.6",
    "@sanity/ui-workshop": "^2.0.3",
    "@storybook/addon-a11y": "^7.4.5",
    "@storybook/addon-docs": "^7.4.5",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-storysource": "^7.4.5",
    "@storybook/addon-themes": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/jest": "^0.2.2",
    "@storybook/manager-api": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/theming": "^7.4.5",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-is": "^18.2.4",
    "@types/refractor": "^3.4.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.0",
    "cypress-real-events": "^1.11.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.14",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "refractor": "^4.8.1",
    "rimraf": "^5.0.1",
    "semantic-release": "^22.0.8",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.5.2",
    "styled-components": "^6.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "react-is": "^18",
    "styled-components": "^5.2 || ^6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/ui.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/ui/issues"
  },
  "keywords": [
    "sanity",
    "ui",
    "components"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "packageManager": "pnpm@8.11.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "release": {
    "extends": "@sanity/semantic-release-preset",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
