{
  "private": true,
  "name": "@sanity/ui",
  "version": "1.0.0-alpha.0",
  "license": "MIT",
  "scripts": {
    "build": "lerna run --parallel build",
    "dev": "lerna run --parallel --stream dev",
    "dev:docs": "lerna run --stream --scope @sanity/ui-docs dev",
    "dev:storybook": "lerna run --stream --scope @sanity/ui-storybook dev",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "type-check": "lerna run type-check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.0",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "3.0.2",
    "typescript": "4.0.2"
  },
  "workspaces": [
    "ui",
    "ui-docs",
    "ui-storybook"
  ]
}
