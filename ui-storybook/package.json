{
  "private": true,
  "name": "sanity-ui-storybook",
  "version": "1.0.0-alpha.0",
  "main": "package.json",
  "scripts": {
    "build": "build-storybook --quiet --config-dir . --output-dir public",
    "clean": "rimraf public",
    "dev": "start-storybook --ci --quiet --config-dir . --port 9009",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx",
    "prebuild": "npm run clean",
    "predev": "rimraf node_modules/.cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "@sanity/ui": "^1.0.0-alpha.7",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-a11y": "^6.0.27",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "react-popper": "^2.2.3",
    "react-refractor": "^2.1.2",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/chance": "^1.1.0",
    "@types/react-is": "^17.0.0",
    "@types/refractor": "^3.0.0",
    "@types/styled-components": "^5.1.4"
  }
}
